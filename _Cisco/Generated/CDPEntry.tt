<# Add_Class(a:="Cisco", d:="Cisco Discovery Protocol Entry") #>
<# Add_Behaviour(n:="Comparable", p:="Fd_Comparable:Index") #>
<# Add_Behaviour(n:="Formattable", p:="Fo_Names:|Fo_Values:{Name}") #>
<# Add_Behaviour(n:="Serialisable") #>
<# Add_Behaviour(n:="Snmp") #>
<# Add_Field(n:="Index", t:="System.Int32", d:="A unique value for each device from which CDP messages are being received.") #>
<# Add_Field(n:="Address", t:="System.Net.IPAddress", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.4.{Parent.Index}.{Index}", d:="The (first) network-layer address of the device's SNMP-agent as reported in the Address TLV of the most recently received CDP message.") #>
<# Add_Field(n:="Description", t:="IOSDetails", f:="NEW", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.5.{Parent.Index}.{Index}", d:="The Version string as reported in the most recent CDP message. The zero-length string indicates no Version field (TLV) was reported in the most recent CDP message.") #>
<# Add_Field(n:="Name", t:="System.String", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.6.{Parent.Index}.{Index}", d:="The Device-ID string as reported in the most recent CDP message.  The zero-length string indicates no Device-ID field (TLV) was reported in the most recent CDP message.") #>
<# Add_Field(n:="InterfaceName", t:="System.String", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.7.{Parent.Index}.{Index}", d:="The Port-ID string as reported in the most recent CDP message. This will typically be the value of the ifName object (e.g., 'Ethernet0'). The zero-length string indicates no Port-ID field (TLV) was reported in the most recent CDP message.") #>
<# Add_Field(n:="Platform", t:="System.String", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.8.{Parent.Index}.{Index}", d:="The Device's Hardware Platform as reported in the most recent CDP message. The zero-length string indicates that no Platform field (TLV) was reported in the most recent CDP message.") #>
<# Add_Field(n:="NativeVlan", t:="System.Int32", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.11.{Parent.Index}.{Index}", d:="The remote device's interface's native VLAN, as reported in the most recent CDP message.  The value 0 indicates no native VLAN field (TLV) was reported in the most recent CDP message.") #>
<# Add_Field(n:="PowerConsumption", t:="PoePower", f:="NEW", m:="Snmp:1.3.6.1.4.1.9.9.23.1.2.1.1.15.{Parent.Index}.{Index}|Snmp_Field:{0}.Parse", d:="The amount of power consumed by remote device, as reported in the most recent CDP message. This object is not instantiated if no Power Consumption field (TLV) was reported in the most recently received CDP message.") #>
<#@ include file="%TEMPLATES_PATH%\Classes\VB_Object.tt" #>