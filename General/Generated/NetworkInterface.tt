<# Add_Class(d:="Network Interface") #>
<# Add_Behaviour(n:="Comparable", p:="Fd_Comparable:Index") #>
<# Add_Behaviour(n:="Formattable", p:="Fo_Names:|Fo_Values:{Name}") #>
<# Add_Behaviour(n:="NetworkInterface") #>
<# Add_Behaviour(n:="Snmp") #>
<# Add_Field(n:="Index", t:="System.Int32", d:="Unique value for each interface. Its value ranges between 1 and the value of ifNumber. The value for each interface must remain constant at least from one re-initialization of the entity's network management system to the next re-initialization.") #>
<# Add_Field(n:="Name", t:="InterfaceName", m:="Snmp:1.3.6.1.2.1.2.2.1.2.{Index}", d:="Unique Name of the Interface.") #>
<# Add_Field(n:="Type", t:="InterfaceType", m:="Snmp:1.3.6.1.2.1.2.2.1.3.{Index}", d:="The type of interface, distinguished according to the physical/link protocol(s) immediately `below' the network layer in the protocol stack.") #>
<# Add_Field(n:="Speed", t:="InterfaceSpeed", f:="NEW", m:="Snmp:1.3.6.1.2.1.2.2.1.5.{Index}|Snmp_Field:{0}.Value", d:="An estimate of the interface's current bandwidth in bits per second.  For interfaces which do not vary in bandwidth or for those where no accurate estimation can be made, this object should contain the nominal bandwidth.") #>
<# Add_Field(n:="PhysicalAddress", t:="System.Net.NetworkInformation.PhysicalAddress", m:="Snmp:1.3.6.1.2.1.2.2.1.6.{Index}", d:="The interface's address at the protocol layer immediately `below' the network layer in the protocol stack.  For interfaces which do not have such an address (e.g., a serial line), this object should contain an octet string of zero length.") #>
<# Add_Field(n:="AdminStatus", t:="InterfaceStatus", m:="Snmp:1.3.6.1.2.1.2.2.1.7.{Index}", d:="The desired state of the interface.") #>
<# Add_Field(n:="Status", t:="InterfaceStatus", m:="Snmp:1.3.6.1.2.1.2.2.1.8.{Index}|Snmp_Transient:True", d:="The current operational state of the interface.") #>
<# Add_Field(n:="Description", t:="System.String", m:="Snmp:1.3.6.1.2.1.31.1.1.1.18.{Index}", d:="This object is an 'alias' name for the interface as  specified by a network manager, and provides a non-volatile 'handle' for the interface.") #>
<# Add_Field(n:="LastChange", t:="TimePeriod", f:="NEW", m:="Snmp:1.3.6.1.2.1.2.2.1.9.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The value of sysUpTime at the time the interface entered its current operational state.  If the current state was entered prior to the last re- initialization of the local network management subsystem, then this object contains a zero value.") #>
<# Add_Field(n:="InOctets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.2.2.1.10.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The total number of octets received on the interface, including framing characters.") #>
<# Add_Field(n:="InUnicastPackets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.2.2.1.11.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The number of packets, delivered by this sub-layer to a higher (sub-)layer, which were not addressed to a multicast or broadcast address at this sub-layer.") #>
<# Add_Field(n:="InBroadcastPackets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.31.1.1.1.3.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The number of packets, delivered by this sub-layer to a higher (sub-)layer, which were addressed to a broadcast address at this sub-layer.") #>
<# Add_Field(n:="InMulticastPackets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.31.1.1.1.2.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The number of packets, delivered by this sub-layer to a higher (sub-)layer, which were addressed to a multicast address at this sub-layer.  For a MAC layer protocol, this includes both Group and Functional addresses.") #>
<# Add_Field(n:="OutOctets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.2.2.1.16.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The total number of octets transmitted out of the interface, including framing characters.") #>
<# Add_Field(n:="OutUnicastPackets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.2.2.1.17.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The total number of packets that higher-level protocols requested be transmitted, and which were not addressed to a multicast or broadcast address at this sub-layer, including those that were discarded or not sent.") #>
<# Add_Field(n:="OutBroadcastPackets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.31.1.1.1.5.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The total number of packets that higher-level protocols requested be transmitted, and which were addressed to a broadcast address at this sub-layer, including those that were discarded or not sent.") #>
<# Add_Field(n:="OutMulticastPackets", t:="DeltaCounter32", f:="NEW", m:="Snmp:1.3.6.1.2.1.31.1.1.1.4.{Index}|Snmp_Field:{0}.Value|Snmp_Transient:True", d:="The total number of packets that higher-level protocols requested be transmitted, and which were addressed to a multicast address at this sub-layer, including those that were discarded or not sent.  For a MAC layer protocol, this includes both Group and Functional addresses.") #>
<#@ include file="%TEMPLATES_PATH%\Classes\VB_Object.tt" #>