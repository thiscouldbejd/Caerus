<# Add_Class(a:="IBM", d:="Class Representing an IBM Power Domain") #>
<# Add_Behaviour(n:="Comparable", p:="Fd_Comparable:Index") #>
<# Add_Behaviour(n:="Serialisable") #>
<# Add_Behaviour(n:="Snmp", p:="Mp_Version:SNMP_VERSION_1") #>
<# Add_Field(n:="Index", t:="System.Int32", d:="Power Domain Sequence Index.") #>
<# Add_Field(n:="State", t:="PowerState", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.3.{Index}", d:="Fuel Gauge Table Status for the power domain.") #>
<# Add_Field(n:="PowerModule1", t:="System.String", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.4.{Index}", d:="Fuel Gauge Table First Power Module capacity in Watts in the power domain. Note:First power module in power domain 1 and 2 is in bay 1 and 3 respectively.") #>
<# Add_Field(n:="PowerModule2", t:="System.String", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.5.{Index}", d:="Fuel Gauge Table Second Power Module capacity in Watts in the power domain. Note:Second power module in power domain 1 and 2 is in bay 2 and 4 respectively.") #>
<# Add_Field(n:="ManagementPolicy", t:="PowerDomainPolicy", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.6.{Index}", d:="Fuel Gauge Table for the Power Management Policy Setting for each power domain. When a 1200W or 1400W power module is installed in the domain the power management policy will not be applicable. In addition the power consumption in this domain will not be monitored.") #>
<# Add_Field(n:="TotalPower", t:="Power", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.7.{Index}", d:="This field displays the total available power for each power domain based on installed power modules and the current Power Management Policy Setting. When a 1200W or 1400W power module is installed in the domain the power management policy will not be applicable. In addition the power consumption in this domain will not be monitored.") #>
<# Add_Field(n:="AllocatedPower", t:="Power", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.8.{Index}", d:="This field displays the power that has been reserved in each power domain. Note:1) Power is always reserved in domain 1 for two management modules and two I/O modules.  2) Power is reserved for I/O modules installed in bay 3 and 4  (Powered on or standby). 3) Power is reserved for each installed Blade. Blades in a standby state (not powered on) will reserve a certain amount power. When a 1200W or 1400W power module is installed in the domain the power management policy will not be applicable. In addition the power consumption in this domain will not be monitored.") #>
<# Add_Field(n:="RemainingPower", t:="Power", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.9.{Index}", d:="This field displays the remaining power available in each domain. Remaining Power =Total Power minus Power In Use When a 1200W or 1400W power module is installed in the domain the power management policy will not be applicable. In addition the power consumption in this domain will not be monitored.") #>
<# Add_Field(n:="UsedPower", t:="Power", m:="Snmp:1.3.6.1.4.1.2.3.51.2.2.10.1.1.1.10.{Index}", d:="This field displays the total power being used in each power domain. When a 1200W or 1400W power module is installed in the domain the power management policy will not be applicable. In addition the power consumption in this domain will not be monitored.") #>
<#@ include file="%TEMPLATES_PATH%\Classes\VB_Object.tt" #>